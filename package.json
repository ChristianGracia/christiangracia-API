{
  "name": "cg-api",
  "version": "0.0.0",
  "description": "TypeScript Node API that serves christiangracia.com",
  "engines": {
    "node": ">= 12.13 <13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/highlight.js": "^10.1.0",
    "@types/jest": "^26.0.24",
    "@types/node": "~12.12",
    "@typescript-eslint/eslint-plugin": "~3.7.0",
    "@typescript-eslint/parser": "~3.7.0",
    "eslint": "~7.5.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-jest": "~23.18.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.0.5",
    "rimraf": "~3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "~26.1.3",
    "tslib": "^2.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsutils": "~3.17.0",
    "typescript": "^4.2"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon -x ts-node src/server.ts",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "test": "jest --config jest.config.json --runInBand",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "lint": "tslint -p tsconfig.json",
    "push-p": "heroku container:push web --app christiangracia-api",
    "release-p": "heroku container:release web --app christiangracia-api",
    "push-s": "heroku container:push web --app christiangracia-api-staging",
    "release-s": "heroku container:release web --app christiangracia-api-staging",
    "rebuild": "docker-compose down && docker-compose build && docker-compose up"
  },
  "author": "Christian Gracia",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/puppeteer": "^5.4.4",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "highlight.js": "^11.4.0",
    "http-errors": "^1.8.1",
    "inline-css": "^3.0.0",
    "morgan": "^1.10.0",
    "puppeteer": "^10.4.0",
    "querystring": "^0.2.1",
    "throng": "^5.0.0",
    "ts-mailgun": "^0.4.3",
    "ts-node": "^8.10.2",
    "winston": "^3.3.3"
  },
  "main": "src/server.js",
  "repository": "git@github.com:ChristianGracia/christiangracia-API.git"
}
